{
  "name": "remix-template-netlify",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:remix": "remix build",
    "build:css": "npm run generate:css -- --minify",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development netlify dev",
    "dev:tailwind": "npm run generate:css -- --watch",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@netlify/functions": "latest",
    "@portabletext/react": "^1.0.6",
    "@remix-run/netlify-edge": "experimental-netlify-edge",
    "@remix-run/react": "experimental-netlify-edge",
    "@remix-run/server-runtime": "experimental-netlify-edge",
    "@sanity/asset-utils": "^1.2.3",
    "@sanity/groq-store": "^0.4.0",
    "@sanity/image-url": "^1.0.1",
    "@tailwindcss/typography": "^0.5.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "isbot": "^3.4.6",
    "picosanity": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@portabletext/types": "^1.0.3",
    "@remix-run/dev": "experimental-netlify-edge",
    "@remix-run/eslint-config": "experimental-netlify-edge",
    "@remix-run/serve": "experimental-netlify-edge",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-syntax-highlighter": "^15.5.4",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.11.0",
    "groq": "^2.29.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
